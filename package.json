{
   "name": "aperture-discord-bot",
   "version": "1.0.0",
   "description": "A discord moderation bot written in JavaScript",
   "main": "dist/src/index.js",
   "scripts": {
      "start": "tsc && node dist/src/index.js",
      "dev": "nodemon dist/src/index.js",
      "watch": "tsc -w -p tsconfig.json",
      "prettier": "prettier . --write",
      "lint": "eslint . --write",
      "prepare": "husky install"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/joennespreuwers/aperture-discord-bot.git"
   },
   "keywords": [
      "discord-bot",
      "discord",
      "discord.js"
   ],
   "author": "Joenne Spreuwers",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/joennespreuwers/aperture-discord-bot/issues"
   },
   "homepage": "https://github.com/joennespreuwers/aperture-discord-bot#readme",
   "dependencies": {
      "discord.js": "^13.6.0"
   },
   "devDependencies": {
      "@types/node": "^17.0.8",
      "@typescript-eslint/eslint-plugin": "^5.9.1",
      "@typescript-eslint/parser": "^5.9.1",
      "eslint": "^8.6.0",
      "husky": "^7.0.0",
      "lint-staged": "^12.1.7",
      "nodemon": "^2.0.15",
      "prettier": "^2.5.1",
      "typescript": "^4.5.4"
   }
}
